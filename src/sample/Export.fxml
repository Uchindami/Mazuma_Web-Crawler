<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="567.0" prefWidth="729.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.DashboardController">
    <children>
        <Label layoutX="14.0" layoutY="162.0" prefHeight="32.0" prefWidth="218.0" text="Enter Path to CSV       :">
            <font>
                <Font name="Verdana" size="18.0"/>
            </font>
        </Label>
        <TextField fx:id="csvPath" layoutX="286.0" layoutY="166.0" prefHeight="25.0" prefWidth="419.0"/>
        <Button fx:id="browseButton" layoutX="443.0" layoutY="225.0" mnemonicParsing="false" onAction="#browseAction"
                prefHeight="25.0" prefWidth="105.0" text="Browse"/>
        <Button fx:id="exportButton" layoutX="600.0" layoutY="225.0" mnemonicParsing="false" onAction="#exportAction"
                prefHeight="25.0" prefWidth="105.0" text="Export"/>
        <Label fx:id="messageBox" alignment="CENTER" contentDisplay="CENTER" layoutX="123.0" layoutY="121.0"
               prefHeight="17.0" prefWidth="538.0">
            <font>
                <Font name="Verdana Bold" size="12.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
